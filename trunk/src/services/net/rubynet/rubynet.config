<?xml version="1.0" encoding="utf-8" ?>
<rubynet
  stop-service-timeout="3000"
  running-mode="interactive"
  prompt="rubynet$: "
  >
  <nohros>
    <providers>
      <provider
        name ="LoggingProvider"
        type="Nohros.Logging.log4net.LegacyLogger, nohros.logging.log4net">
      </provider>
    </providers>

    <xml-elements>
      <log4net>
        <appender name="Console" type="log4net.Appender.ConsoleAppender">
          <layout type="log4net.Layout.PatternLayout">
            <conversionPattern value="[%date %-5level/%thread] %message%newline %exception"/>
          </layout>
        </appender>

        <root>
          <level value="DEBUG"/>
          <appender-ref ref="Console"/>
        </root>
      </log4net>
    </xml-elements>
  </nohros>
</rubynet>